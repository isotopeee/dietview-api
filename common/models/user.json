{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "injectOptionsFromRemoteContext": true,
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "account": {
      "type": "object",
      "required": true
    },
    "messages": {
      "type": [
        "object"
      ]
    },
    "vitals": {
      "type": "object"
    },
    "activeSubscription": {
      "type": "object"
    },
    "subscriptions": {
      "type": [
        "object"
      ]
    },
    "created": {
      "type": "date"
    },
    "lastUpdated": {
      "type": "date"
    },
    "status": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "changePassword"
    }
  ],
  "methods": {}
}
