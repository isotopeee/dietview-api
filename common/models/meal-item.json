{
  "name": "MealItem",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SoftDelete": true,
    "AuditLog": true,
    "UniqueValidator": {
      "properties": [
        "name",
        {
          "field": "name",
          "message": "Tne name of the meal-item is already existing in the system."
        }
      ]
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "calories": {
      "type": "number",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "remarks": {
      "type": "string"
    },
    "status": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "meals": {
      "type": "hasAndBelongsToMany",
      "model": "Meal",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
